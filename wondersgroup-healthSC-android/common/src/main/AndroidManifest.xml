<manifest package="com.wondersgroup.hs.healthcloud.common"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity
            android:name="com.wondersgroup.hs.healthcloud.common.view.photopick.PhotoPickActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="${applicationId}.PICK_PHOTO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name="com.wondersgroup.hs.healthcloud.common.view.photopick.PhotoViewActivity" />

    </application>

</manifest>
